<odoo>
    <record id="view_inventory_session_line_import_wizard_form" model="ir.ui.view">
        <field name="name">inventory.session.line.import.wizard.form</field>
        <field name="model">inventory.session.line.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Session Lines">
                <group>
                    <field name="session_id" readonly="1"/>
                    <field name="file" widget="binary"/>
                    <field name="file_name"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import_lines" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
